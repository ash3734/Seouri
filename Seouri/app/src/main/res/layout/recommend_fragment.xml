<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="sopt.seouri.recommend.RecommendFragment">
    <RelativeLayout
        android:id="@+id/reco_1"
        android:background="@drawable/bukchon"
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/reco_2"
        android:background="@drawable/mukbang"
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/reco_3"
        android:layout_weight="1"
        android:background="@drawable/healing"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/reco_4"
        android:layout_weight="1"
        android:background="@drawable/coffee"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/reco_5"
        android:layout_weight="1"
        android:background="@drawable/sul"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    </RelativeLayout>

</LinearLayout>